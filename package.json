  "scripts": {
    "build": "cp ./content/problems.schema.json ./static/problems.schema.json && gatsby build",
    "dev": "gatsby develop",
    "dev:optimized": "yarn build:css-once && concurrently \"gatsby develop\" \"yarn build:css-watch\"",
    "develop": "gatsby develop",
    "format": "prettier --write .",
    "lint": "npm run eslint",
    "eslint": "eslint --fix --ext .jsx --ext .js --ext .ts --ext .tsx --cache src",
    "check-ts-errors": "tsc --noEmit",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "check-links": "blc http://localhost:9000 -qorf --exclude train.usaco.org --exclude quora.com --exclude tablesgenerator.com --exclude judge.yosupo.jp --exclude kenkoooo.com --requests 10",
    "functions:deploy": "firebase deploy --only functions",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token 6f3c58ad349c --exit-zero-on-changes",
    "lint-staged": "lint-staged",
    "build:css-once": "tailwindcss -i ./src/styles/main.css -o ./build.css",
    "build:css-watch": "tailwindcss -i ./src/styles/main.css -o ./build.css --watch"
  },
